{
    "min_version": "v0.0.0",
    "servers": [
        {
            "listen_on": "0.0.0.0:443",
            "filters": [
                {
                    "protocol": "TLS",
                    "rules": [
                        {
                            "spec": "SNI gaukas.wang",
                            "action": "FORWARD",
                            "to_addr": "gaukas.wang:443"
                        },
                        {
                            "spec": "SNI google.com",
                            "action": "FORWARD",
                            "to_addr": "google.com:443"
                        },
                        {
                            "spec": "CATCHALL",
                            "action": "REJECT"
                        }
                    ]
                },
                {
                    "protocol": "SSH",
                    "rules": [
                        {
                            "spec": "CATCHALL",
                            "action": "REJECT"
                        }
                    ]
                }
            ]
        },
        {
            "listen_on": "0.0.0.0:22",
            "filters": [
                {
                    "protocol": "SSH",
                    "rules": [
                        {
                            "spec": "CATCHALL",
                            "action": "FORWARD", 
                            "to_addr": "127.0.0.1:22122"
                        }
                    ]
                }
            ]
        }
    ]
}